<h1><%= @place["name"]%></h1>

<ul>
    <% for post in @posts %>
    <li>
        <h2><%= post["title"] %> </h2>
        <p><%= post["posted_on"] %>
        <p><%= post["description"] %> </p>
    </li>
    <% end %>

</ul>

<a href ="/posts/new?place_id=<%= @place["id"] %>"><button>New Post</button></a>
<p><a href ="/places">Back to Places</a></p>